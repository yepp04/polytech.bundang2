package test;

import java.util.Scanner;

public class Solut {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		while (true) {
			int b = 0;
			while (true) {
				if (a == b) {
					break;
				}
				System.out.print("*");
			}
			b++;
			System.out.println();
		}
	}

}